\ProvidesExplFile{soranus9pt++++.clo}{2018/10/11}{0.1.0}{Soranus class 9pt++++ size option.}

\EditInstance {fontsize} {miniscule}
  {
    point-size          = 4.5pt,
    baseline-separation = 6.25pt
  }

\EditInstance {fontsize} {tiny}
  {
    point-size          = 5.5pt,
    baseline-separation = 7.75pt
  }

\EditInstance {fontsize} {scriptsize}
  {
    point-size          = 6.25pt,
    baseline-separation = 8.75pt
  }

\EditInstance {fontsize} {footnotesize}
  {
    point-size          = 7.25pt,
    baseline-separation = 10.25pt
  }

\EditInstance {fontsize} {small}
  {
    point-size          = 8.0pt,
    baseline-separation = 11.25pt
  }

\EditInstance {fontsize} {normalsize}
  {
    point-size          = 9pt,
    baseline-separation = 12.5pt
  }

\EditInstance {fontsize} {large}
  {
    point-size          = 10.75pt,
    baseline-separation = 15.0pt
  }

\EditInstance {fontsize} {Large}
  {
    point-size          = 12.5pt,
    baseline-separation = 17.5pt
  }

\EditInstance {fontsize} {LARGE}
  {
    point-size          = 14.5pt,
    baseline-separation = 20.25pt
  }

\EditInstance {fontsize} {huge}
  {
    point-size          = 21.5pt,
    baseline-separation = 30.0pt
  }

\EditInstance {fontsize} {Huge}
  {
    point-size          = 25.25pt,
    baseline-separation = 35.25pt
  }

\EditInstance {fontsize} {HUGE}
  {
    point-size          = 28.75pt,
    baseline-separation = 40.25pt
  }

\dim_gset:Nn \g__soranus_top_skip {9pt}
\dim_gset:Nn \g__soranus_max_depth_skip {4.5pt}
