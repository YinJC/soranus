\ProvidesExplFile{soranus14pt++.clo}{2018/10/11}{0.1.0}{Soranus class 14pt++ size option.}

\EditInstance {fontsize} {miniscule}
  {
    point-size          = 7.0pt,
    baseline-separation = 9.0pt
  }

\EditInstance {fontsize} {tiny}
  {
    point-size          = 8.5pt,
    baseline-separation = 11.0pt
  }

\EditInstance {fontsize} {scriptsize}
  {
    point-size          = 9.75pt,
    baseline-separation = 12.75pt
  }

\EditInstance {fontsize} {footnotesize}
  {
    point-size          = 11.25pt,
    baseline-separation = 14.5pt
  }

\EditInstance {fontsize} {small}
  {
    point-size          = 12.5pt,
    baseline-separation = 16.25pt
  }

\EditInstance {fontsize} {normalsize}
  {
    point-size          = 14pt,
    baseline-separation = 18.25pt
  }

\EditInstance {fontsize} {large}
  {
    point-size          = 16.75pt,
    baseline-separation = 21.75pt
  }

\EditInstance {fontsize} {Large}
  {
    point-size          = 19.5pt,
    baseline-separation = 25.25pt
  }

\EditInstance {fontsize} {LARGE}
  {
    point-size          = 22.5pt,
    baseline-separation = 29.25pt
  }

\EditInstance {fontsize} {huge}
  {
    point-size          = 33.5pt,
    baseline-separation = 43.5pt
  }

\EditInstance {fontsize} {Huge}
  {
    point-size          = 39.25pt,
    baseline-separation = 51.0pt
  }

\EditInstance {fontsize} {HUGE}
  {
    point-size          = 44.75pt,
    baseline-separation = 58.25pt
  }

\dim_gset:Nn \g__soranus_top_skip {14pt}
\dim_gset:Nn \g__soranus_max_depth_skip {7.0pt}
